{
  "version": 1,
  "include": ["/*"],
  "exclude": ["/api/*", "/proxy/*", "/vps/*"],
  "headers": {
    "/*": {
      "Cache-Control": "public, max-age=0, must-revalidate"
    },
    "/assets/*": {
      "Cache-Control": "public, max-age=31536000, immutable"
    },
    "/*.js": {
      "Cache-Control": "public, max-age=31536000, immutable"
    },
    "/*.css": {
      "Cache-Control": "public, max-age=31536000, immutable"
    },
    "/*.woff2": {
      "Cache-Control": "public, max-age=31536000, immutable"
    }
  },
  "redirects": {
    "/": {
      "destination": "/index.html",
      "code": 200
    }
  }
}
